    "@typescript-eslint/parser": "^6.2.0",
    "eslint": "^8.46.0",
    "express": "^4.17.0",
    "typescript": "^5.1.6",
    "vite": "^4.4.0",
    "vite-plugin-dts": "^3.4.0",
    "vite-plugin-eslint": "^1.8.0",
    "vitest": "^0.33.0"
  }
